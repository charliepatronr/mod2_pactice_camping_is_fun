<h1> Add a new Camper!!1 </h1>

<%if flash[:errors] %>
    <% flash[:errors].each do |error|%>
        <%= error %><br>
    <%end%>
<%end%>


<%= form_for @camper do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name%><br><br>
    <%= f.label :age %>
    <%=f.number_field :age%><br><br>
    <%= f.submit%>
<%end%>
